<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta name="description" content="<%= meta.description %>"/>
    <meta name="keywords" content="<%= meta.keywords %>"/>
    <meta name="author" content="<%= meta.author %>"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
    <title><%= title %></title>
    <link rel="shortcut icon" href="/favicon.ico"/>
    <link rel="stylesheet" type="text/css" href="/themes/<%= site.theme %>/css/style.css"/>
    <link rel="stylesheet" type="text/css" href="/themes/<%= site.theme %>/css/scrollUp.css"/>
    <link rel="stylesheet" type="text/css" href="/themes/<%= site.theme %>/css/prettify-solarized-dark.css"/>
    <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.css"/>
  </head>
  <body>
    <div id="container">
      <div id="wrap">
        <header id="header">
          <div id="banner">
          </div>
          <div id="header-outer" class="outer">
            <div id="header-title" class="inner">
              <div class="logo">
                <img src="<%= profile.avatar %>" alt="Profile Picture">
              </div>
              <div id="title">
                <%= title %>
              </div>
              <ul class="my-socials">
                <li>
                  <a href="<%= profile.github %>" class="github" target="_blank">
                    <i class="fa fa-github">
                    </i>
                  </a>
                </li>
                <li>
                  <a href="<%= profile.weibo %>" class="weibo" target="_blank">
                    <i class="fa fa-weibo">
                    </i>
                  </a>
                </li>
                <li>
                  <a href="mailto:<%= profile.email %>" class="email" target="_blank" title="Email Me">
                    <i class="fa fa-envelope">
                    </i>
                  </a>
                </li>
              </ul>
            </div>
          </div>
          <div id="header-inner" class="">
            <nav id="main-nav">
              <a id="main-nav-toggle" class="nav-icon">
              </a>
              <!-- <a class="main-nav-link" href="/">首页</a>
              <a class="main-nav-link" href="/archives">归档</a>
              -->
            </nav>
            <nav id="title-nav" style="display: none;">
              <img src="<%= profile.avatar %>" alt="Profile Picture">
              <a href="/">
                <%= subtitle %>
              </a>
              <!-- <span id="title-nav-socials">
              <a href="https://github.com/henryhuang" class="github" target="_blank">
              <i class="fa fa-github"></i>
              </a>
              <a href="http://weibo.com/626242034" class="weibo" target="_blank">
              <i class="fa fa-weibo"></i>
              </a>
              <a href="mailto:h1886@outlook.com" class="email" target="_blank" title="Email Me">
              <i class="fa fa-envelope"></i>
              </a>
              </span>
              -->
            </nav>
            <nav id="sub-nav">
              <a id="nav-search-btn" class="nav-icon" title="Search">
              </a>
            </nav>
            <div id="search-form-wrap">
              <form action="http://www.baidu.com/baidu" method="get" accept-charset="utf-8"
              class="search-form">
                <input type="search" name="word" maxlength="20" class="search-form-input"
                placeholder="搜索">
                <input type="submit" value="" class="search-form-submit">
                <input name="tn" type="hidden" value="bds">
                <input name="cl" type="hidden" value="3">
                <input name="ct" type="hidden" value="2097152">
                <input type="hidden" name="si" value="www.cnhalo.com">
              </form>
            </div>
          </div>
          <div class="site-nav" style="display: none;">
            <ul>
              <li>
                <a href="/">
                  首页
                </a>
              </li>
              <li>
                <a href="/archives">
                  归档
                </a>
              </li>
            </ul>
          </div>
        </header>
